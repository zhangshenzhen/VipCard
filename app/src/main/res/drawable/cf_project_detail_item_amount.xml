<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_selected="true"  android:color="@color/cf_default_text" >
        <shape
            android:shape="rectangle">
            <corners android:radius="5dp" />
            <stroke android:color="@color/cf_default_text" android:width="1dp" />
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item android:color="@color/black_9" >
        <shape
            android:shape="rectangle">
            <corners android:radius="5dp" />
            <stroke android:color="@color/black_9" android:width="1dp" />
            <solid android:color="@color/white" />
        </shape>
    </item>
</selector>
